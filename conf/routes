# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# webjars route!
GET        /webjars/*file                                  controllers.WebJarAssets.at(file)
GET        /                                               controllers.Application.index
GET        /:applicationId                                 controllers.Application.applicationDetails(applicationId)
GET        /:applicationId/applications                    controllers.Application.applicationSubApps(applicationId)
GET        /:applicationId/applications/:subAppName        controllers.SubApplication.subAppIndex(applicationId, subAppName)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                                   controllers.Assets.versioned(path="/public", file: Asset)
